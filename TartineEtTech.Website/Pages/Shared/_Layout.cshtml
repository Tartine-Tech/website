<!DOCTYPE html>
<html lang="fr">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <title>@ViewData["Title"] | Tartine &amp; Tech</title>

    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700&amp;subset=latin" rel="stylesheet">
    <link href="~/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/css/nifty.min.css" rel="stylesheet">
    <link href="~/lib/premium/icon-sets/icons/line-icons/premium-line-icons.min.css" rel="stylesheet">
    <link href="~/lib/premium/icon-sets/icons/solid-icons/premium-solid-icons.min.css" rel="stylesheet">
    <link href="~/css/pace.min.css" rel="stylesheet">
    <script src="~/js/pace.min.js"></script>
    <link href="~/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/css/themes/type-full/theme-dark-full.min.css" rel="stylesheet">
    <link href="~/css/custom.css" rel="stylesheet">
</head>


<body>
    <div id="container" class="effect aside-float aside-bright mainnav-lg">
        <header id="navbar">
            <div id="navbar-container" class="boxed">
                <div class="navbar-header">
                    <a asp-page="Index" class="navbar-brand">
                        <img src="img/logo.png" alt="Tartine & Tech Logo" class="brand-icon">
                        <div class="brand-title">
                            <span class="brand-text">Tartine &amp; Tech</span>
                        </div>
                    </a>
                </div>
                <div class="navbar-content clearfix">
                    <ul class="nav navbar-top-links">
                        <li class="tgl-menu-btn">
                            <a class="mainnav-toggle" href="#">
                                <i class="pli-list-view icon-lg"></i>
                            </a>
                        </li>
                        <li>
                    </ul>
                </div>
            </div>
        </header>


        <div class="boxed">
            <div id="content-container">
                @RenderBody()
            </div>

            @await Html.PartialAsync("_Nav")
        </div>

        <footer id="footer">
            <p class="pad-lft">&#0169; @DateTime.Now.Year <a href="https://www.senseoftech.be">Sense of Tech</a> &amp; Space Y</p>
        </footer>

    </div>

    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/nifty.min.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>